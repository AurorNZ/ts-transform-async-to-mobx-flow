{
  "name": "ts-transform-async-to-mobx-flow",
  "description": "Typescript transformer for converting async functions into generators wrapped with mobx.flow",
  "repository": "git@github.com:aurornz/ts-transform-async-to-mobx-flow.git",
  "author": "Auror <developers@auror.co>",
  "private": true,
  "license": "MIT",
  "keywords": [
    "mobx",
    "typescript",
    "typescript-transformer"
  ],
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.13",
    "jest": "^26.5.6",
    "lerna": "^4.0.0",
    "mobx": "^4.5.0",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "typescript": "^5.0.4",
    "typescript4": "npm:typescript@4.7.x"
  },
  "scripts": {
    "test": "lerna run test"
  },
  "workspaces": [
    "packages/*"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.|\\.(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
